<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qq937009442.threekindomshuarongroad">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChooseLevel"
            android:label="@string/title_activity_choose_level"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.qq937009442.threekindomshuarongroad.MainActivity" />
        </activity>
        <activity
            android:name=".Game_1"
            android:label="@string/lable_game_1" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".Game_2"
            android:label="@string/lable_game_2" />
        <activity
            android:name=".Game_3"
            android:label="@string/lable_game_3" />
        <activity
            android:name=".Game_4"
            android:label="@string/lable_game_4" />
        <activity android:name=".aboutAuthor"
            android:label="关于"></activity>
    </application>

</manifest>